{
    "compilerOptions": {
      "jsx": "react-jsx",
      "jsxImportSource": "react",
      "moduleResolution": "bundler",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "target": "ESNext",
      "module": "ESNext",
      "types": ["vite/client"],
      "paths": {
        "@/*": ["./src/*"]
      }
    }
  }
  